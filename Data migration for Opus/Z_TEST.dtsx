                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	'OPUS' AS co_cd,
	seq_type AS opt_key_cd,
	cofig_name AS opt_desc,
	pre_fix AS opt_val,
	'admin' AS cre_usr_id,
	'admin' AS upd_usr_id,
	GETDATE() AS cre_dt,
	GETDATE() AS upd_dt
FROM 
(
	SELECT 
	seq_type 
	,'SYSTEM OFFICE' AS cofig_name
	,pre_fix
  FROM
       TB_SEQ             
    WHERE SEQ_TYPE = 'SYS_OFC_CD'
UNION ALL    
SELECT 
	seq_type 
	,'ACCOUNTING BEGINNING DATE' AS cofig_name
	,CASE WHEN pre_fix != null or pre_fix != '' THEN convert(varchar, CONVERT(DATETIME , CONCAT(pre_fix,'01'), 103), 101) ELSE '' END AS pre_fix
  FROM
       TB_SEQ             
    WHERE SEQ_TYPE = 'BEG_YYYYMM'    
UNION ALL    
SELECT 
	seq_type 
	,'YEAR CLEAR MONTH' AS cofig_name
	,pre_fix
  FROM
       TB_SEQ             
    WHERE SEQ_TYPE = 'YEAR_CLS_MM' 
) A</property>
